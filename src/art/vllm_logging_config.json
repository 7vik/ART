{
    "formatters": {
        "vllm": {
            "class": "vllm.logging_utils.NewLineFormatter",
            "datefmt": "%m-%d %H:%M:%S",
            "format": "%(levelname)s %(asctime)s [%(filename)s:%(lineno)d] %(message)s"
        }
    },
    "handlers": {
        "vllm": {
            "class": "logging.FileHandler",
            "formatter": "vllm",
            "level": "INFO",
            "filename": "./logs/vllm.log"
        }
    },
    "loggers": {
        "vllm": {
            "handlers": [
                "vllm"
            ],
            "level": "DEBUG",
            "propagate": false
        }
    },
    "version": 1,
    "disable_existing_loggers": false
}